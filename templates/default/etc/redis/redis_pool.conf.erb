###
# Generated by Chef for [<%= node[:fqdn] %>]
# Environment: <%= node.chef_environment %>
###

<%- @config.sort.each do |key, values| -%>
<%- [*values].each do |value| -%>
<%- next if value.nil? -%>
<%= key %> <%=
  case value
  when String
    value
  when TrueClass
    1
  when FalseClass
    0
  else
    value
  end
%>
<%- end -%>
<%- end -%>
